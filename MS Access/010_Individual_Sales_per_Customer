SELECT [011_ForgalomFejenkent].Year, [011_ForgalomFejenkent].Month, [00_months].month_name, [011_ForgalomFejenkent].user_full_name, [012_KremeladasFejenkent]!SumOfsum_price1/[011_ForgalomFejenkent]!CountOfcard_event_id AS KER, [012_KremeladasFejenkent].SumOfsum_price1, [011_ForgalomFejenkent].CountOfcard_event_id
FROM 00_months INNER JOIN (011_ForgalomFejenkent INNER JOIN 012_KremeladasFejenkent ON ([011_ForgalomFejenkent].year = [012_KremeladasFejenkent].year) AND ([011_ForgalomFejenkent].month = [012_KremeladasFejenkent].month) AND ([011_ForgalomFejenkent].user_full_name = [012_KremeladasFejenkent].user_full_name)) ON [00_months].month_id = [011_ForgalomFejenkent].month
WHERE ((([011_ForgalomFejenkent].Year)>2017) AND (([011_ForgalomFejenkent].user_full_name)="Enci")) OR ((([011_ForgalomFejenkent].Year)>2017) AND (([011_ForgalomFejenkent].user_full_name)="Orsi")) OR ((([011_ForgalomFejenkent].Year)>2017) AND (([011_ForgalomFejenkent].user_full_name)="Kata")) OR ((([011_ForgalomFejenkent].Year)>2017) AND (([011_ForgalomFejenkent].user_full_name)="Anna")) OR ((([011_ForgalomFejenkent].Year)>2017) AND (([011_ForgalomFejenkent].user_full_name)="Reni"));

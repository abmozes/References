SELECT [020_EladottKremLista].Year, [020_EladottKremLista].Month, [020_EladottKremLista].user_full_name, [020_EladottKremLista].product_name1, [020_EladottKremLista].product_name2, [020_EladottKremLista].abs_amount, [020_EladottKremLista].SumOfsum_price, [010_KER].KER, [010_KER].SumOfsum_price1, [010_KER].CountOfcard_event_id
FROM 00_months INNER JOIN (010_KER INNER JOIN 020_EladottKremLista ON ([010_KER].user_full_name = [020_EladottKremLista].user_full_name) AND ([010_KER].month = [020_EladottKremLista].month) AND ([010_KER].year = [020_EladottKremLista].year)) ON [00_months].month_id = [020_EladottKremLista].month;
